<h1>Burgers!</h1>
<img src="https://freesvg.org/img/Gerald_G_Fast_Food_Lunch_Dinner_%28FF_Menu%29_6.png" alt="burger">
  
</object>
<div class="menu">

  <div class="items">
<h2>Burgers that arent eaten!</h2>

<ul>
  {{#each burgers}}
    {{#unless devoured}}
      {{> burgers/burger-block devoured=true}}
    {{/unless}}
  {{/each}}
</ul>
</div>
<div class="items">
    <h2>Burgers that are eaten!</h2>


<ul>
  {{#each burgers}}
    {{#if devoured}}
      {{> burgers/burger-block devoured=false}}
    {{/if}}
  {{/each}}
</ul>
              </div>
        </div>
<h2>Add a Burger</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">Burger Name:</label>
    <input type="text" id="ca" name="name">
  </div>

  <div class="form-group">
    <label for="devoured">eaten?</label><br>
    <input type="radio" name="devoured" value="1" checked> Eaten<br>
    <input type="radio" name="devoured" value="0"> Not
  </div>


  <button type="submit">Add Burger</button>
</form>